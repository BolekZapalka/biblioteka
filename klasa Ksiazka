public class Ksiazka {
    int liczbaKopii;
    String tytul;
    String autor;
    Boolean wypozyczona;

    //konstruktor ksiazki:
    public Ksiazka(String nowyTytul, String nowyAutor, Boolean czyWypozyczona, int liczbaKopii) {
        autor = nowyAutor;
        tytul = nowyTytul;
        wypozyczona = czyWypozyczona;
        this.liczbaKopii = liczbaKopii;
    }



    public String getTytul() {
        return tytul;
    }

    public void setTytul(String tytul) {
        this.tytul = tytul;
    }

    public String getAutor() {
        return autor;
    }

    public void setAutor(String autor) {
        this.autor = autor;
    }

    public Boolean getWypozyczona() {
        return wypozyczona;
    }

    public void setWypozyczona(Boolean wypozyczona) {
        this.wypozyczona = wypozyczona;
    }

    public int getliczbaKopii() { return liczbaKopii; }

    public void setLiczbaKopii(int liczbaKopii) { this.liczbaKopii = liczbaKopii; }

    @Override
    public String toString() {
        return "Ksiazka{" +

                " tytul='" + tytul + '\'' +
                ", autor='" + autor + '\'' +
                ", wypozyczona=" + wypozyczona +
                ", liczbaKopii=" + liczbaKopii +
                '}';
    }
}
