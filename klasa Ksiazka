public class Ksiazka {
    String tytul;
    String autor;
    Boolean wypozyczona;

    //konstruktor ksiazki:
    public Ksiazka(String nowyTytul, String nowyAutor, Boolean czyWypozyczona) {
        this.autor = nowyAutor;
        this.tytul = nowyTytul;
        this.wypozyczona = czyWypozyczona;
    }
    

    public String getTytul() {
        return tytul;
    }

    public void setTytul(String tytul) {
        this.tytul = tytul;
    }

    public String getAutor() {
        return autor;
    }

    public void setAutor(String autor) {
        this.autor = autor;
    }

    public Boolean getWypozyczona() {
        return wypozyczona;
    }

    public void setWypozyczona(Boolean wypozyczona) {
        this.wypozyczona = wypozyczona;
    }

    @Override
    public String toString() {
        return "Ksiazka{" + "tytul: " + tytul + " " + "autor: " + autor + "wypozyczona " + wypozyczona + "}";
    }

}
